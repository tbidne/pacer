MkChartRequests
  { chartRequests = fromList
    [ MkChartRequest
      { chartType = Nothing
      , description = Just "This is some activities plotted by distance."
      , filters = []
      , title = "Activities by distance"
      , unit = Nothing
      , yAxis = YAxisDistance
      , y1Axis = Nothing
      }
    , MkChartRequest
      { chartType = Nothing
      , description = Just "The same activities as before, but in miles."
      , filters = []
      , title = "Activities by distance and pace"
      , unit = Just Mile
      , yAxis = YAxisDistance
      , y1Axis = Just YAxisPace
      }
    , MkChartRequest
      { chartType = Nothing
      , description = Nothing
      , filters =
        [ Atom
          ( FilterLabel
            ( FilterElemElem FilterElemElemOpEq "marathon" )
          )
        , Atom
          ( FilterDate FilterOpGte
            ( MomentYear
              ( MkYear
                { unYear = UnsafeInterval
                  ( Closed 1950 )
                  ( Closed 2099 ) 2024
                }
              )
            )
          )
        ]
      , title = "Marathons"
      , unit = Nothing
      , yAxis = YAxisDuration
      , y1Axis = Just YAxisPace
      }
    , MkChartRequest
      { chartType = Nothing
      , description = Nothing
      , filters =
        [ Atom
          ( FilterLabel
            ( FilterElemElem FilterElemElemOpEq "official" )
          )
        , Atom
          ( FilterLabel
            ( FilterElemElem FilterElemElemOpNeq "marathon" )
          )
        ]
      , title = "Official non-marathons"
      , unit = Nothing
      , yAxis = YAxisPace
      , y1Axis = Nothing
      }
    , MkChartRequest
      { chartType = Nothing
      , description = Nothing
      , filters =
        [ Or
          ( Atom
            ( FilterLabel
              ( FilterElemElem FilterElemElemOpEq "half-marathon" )
            )
          )
          ( Atom
            ( FilterLabel
              ( FilterElemElem FilterElemElemOpEq "marathon" )
            )
          )
        ]
      , title = "Marathons and half-marathons"
      , unit = Nothing
      , yAxis = YAxisPace
      , y1Axis = Nothing
      }
    , MkChartRequest
      { chartType = Just ChartTypeDefault
      , description = Nothing
      , filters =
        [ Atom
          ( FilterDistance FilterOpGte
            ( MkSomeDistance SKilometer
              ( MkDistance
                ( UnsafePositive 25.0 ) Kilometer
              )
            )
          )
        ]
      , title = "Long activities"
      , unit = Nothing
      , yAxis = YAxisDistance
      , y1Axis = Just YAxisPace
      }
    , MkChartRequest
      { chartType = Just ( ChartTypeSum ChartSumWeek )
      , description = Nothing
      , filters = []
      , title = "Runs by week"
      , unit = Nothing
      , yAxis = YAxisDistance
      , y1Axis = Nothing
      }
    ]
  , garminSettings = Just
    ( MkGarminSettings { distanceUnit = Just Kilometer } )
  }
