MkChartRequests
    { unChartRequests = fromList
        [ MkChartRequest
            { description = Just "This is some runs plotted by distance."
            , filters = []
            , title = "Runs by distance"
            , unit = Nothing
            , yAxis = YAxisDistance
            , y1Axis = Nothing
            }
        , MkChartRequest
            { description = Just "The same runs as before, but in miles."
            , filters = []
            , title = "Runs by distance and pace"
            , unit = Just Mile
            , yAxis = YAxisDistance
            , y1Axis = Just YAxisPace
            }
        , MkChartRequest
            { description = Nothing
            , filters =
                [ Atom
                    ( FilterLabel "marathon" )
                ]
            , title = "Marathons"
            , unit = Nothing
            , yAxis = YAxisDuration
            , y1Axis = Just YAxisPace
            }
        , MkChartRequest
            { description = Nothing
            , filters =
                [ Atom
                    ( FilterLabel "official" )
                , Not
                    ( Atom
                        ( FilterLabel "marathon" )
                    )
                ]
            , title = "Official non-marathons"
            , unit = Nothing
            , yAxis = YAxisPace
            , y1Axis = Nothing
            }
        , MkChartRequest
            { description = Nothing
            , filters =
                [ Or
                    ( Atom
                        ( FilterLabel "half-marathon" )
                    )
                    ( Atom
                        ( FilterLabel "marathon" )
                    )
                ]
            , title = "Marathons and half-marathons"
            , unit = Nothing
            , yAxis = YAxisPace
            , y1Axis = Nothing
            }
        , MkChartRequest
            { description = Nothing
            , filters =
                [ Atom
                    ( FilterDistance MkFilterOp >= _
                        ( MkSomeDistance SKilometer
                            ( MkDistance
                                ( UnsafePositive 25.0 ) Kilometer
                            )
                        )
                    )
                ]
            , title = "Long runs"
            , unit = Nothing
            , yAxis = YAxisDistance
            , y1Axis = Just YAxisPace
            }
        ]
    }
