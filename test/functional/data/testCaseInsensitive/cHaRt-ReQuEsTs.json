{
  "garmin": {
    "unit": "km"
  },
  "charts": [
    {
      "title": "Runs by distance",
      "y-axis": "distance",
      "description": "This is some runs plotted by distance."
    },
    {
      "title": "Runs by distance and pace",
      "y-axis": "distance",
      "y1-axis": "pace",
      "description": "The same runs as before, but in miles.",
      "unit": "mi"
    },
    {
      "title": "Marathons",
      "filters": [
        "label marathon",
        "datetime >= 2024"
      ],
      "y-axis": "duration",
      "y1-axis": "pace"
    },
    {
      "title": "Official non-marathons",
      "filters": [
        "label official",
        "not label marathon"
      ],
      "y-axis": "pace"
    },
    {
      "title": "Marathons and half-marathons",
      "filters": [
        "label half-marathon or label marathon"
      ],
      "y-axis": "pace"
    },
    {
      "title": "Long runs",
      "filters": [
        "distance >= 25 km"
      ],
      "y-axis": "distance",
      "y1-axis": "pace"
    }
  ]
}
